<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="d_user" catalog="dangdang"
		class="com.tarena.entity.User">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="d_receive_address" catalog="dangdang"
		class="com.tarena.entity.Address">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="d_category" catalog="dangdang"
		class="com.tarena.entity.Category">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="d_product" catalog="dangdang"
		class="com.tarena.entity.Product">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
<table name="d_book" catalog="dangdang" class="com.tarena.entity">
	<primary-key>
		<generator class="identity"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>